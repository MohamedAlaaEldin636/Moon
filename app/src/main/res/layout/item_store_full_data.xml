<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"

  tools:ignore="ContentDescription">

  <data>

  </data>

  <androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/constraintLayout"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"

    android:layout_margin="8dp"

    android:layout_gravity="center"

    android:background="@drawable/dr_rect_5"

    android:padding="16dp"
    android:clipToPadding="false">

    <ImageView
      android:id="@+id/imageImageView"

      android:layout_width="wrap_content"
      android:layout_height="wrap_content"

      android:src="@drawable/ic_store_data_4"

      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toStartOf="parent" />

    <ImageView
      android:id="@+id/notCompleteImageView"

      android:layout_width="wrap_content"
      android:layout_height="wrap_content"

      android:src="@drawable/ic_not_complete_4"

      app:layout_constraintTop_toTopOf="@id/imageImageView"
      app:layout_constraintBottom_toTopOf="@id/imageImageView"
      app:layout_constraintStart_toStartOf="@id/imageImageView"
      app:layout_constraintEnd_toStartOf="@id/imageImageView"/>

    <ImageView
      android:id="@+id/arrowImageView"

      android:layout_width="wrap_content"
      android:layout_height="wrap_content"

      android:src="@drawable/ic_arrow_store_info_4"

      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
      android:id="@+id/textTextView"

      android:layout_width="0dp"
      android:layout_height="wrap_content"

      style="@style/TextTajawalMedium14"
      android:textSize="15sp"
      tools:text="بيانات المتجر"
      android:textDirection="locale"

      android:layout_marginStart="13dp"
      android:layout_marginEnd="13dp"

      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintStart_toEndOf="@id/imageImageView"
      app:layout_constraintEnd_toStartOf="@id/arrowImageView"/>

  </androidx.constraintlayout.widget.ConstraintLayout>

</layout>
